# OpenAI API設定
OPENAI_API_KEY=your_openai_api_key_here

# ログ設定
LOG_LEVEL=INFO

# エージェント設定
AGENT_COUNT=1  # エンジニア/テスターエージェントの初期数

# LLM設定
MODEL_NAME=gpt-4o  # 使用するLLMモデル名

# データベース設定（開発環境）
# 本番環境ではNeonなどのサービスを使用
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/ai_team_dev

# ストレージ設定（開発環境）
# 本番環境ではGCSなどのサービスを使用
STORAGE_PATH=./storage

# Web検索API設定（オプション）
